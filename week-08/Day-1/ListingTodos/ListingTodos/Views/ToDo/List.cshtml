@model System.Collections.Generic.List<ListingTodos.Models.ToDo>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>ToDo CRUD App</title>
</head>
<body>
    <h1>Todos for today</h1>
    <ul>
        @{foreach (var Todo in Model)
            {
                <li>@Todo.Title</li>
            }

        }

        <li>Start the day</li>
        <li>Finish H2 workshop1</li>
        <li>Finish JPA workshop2</li>
        <li>Create a CRUD project</li>
    </ul>
</body>
</html>
